{
    "version": 2,
    "public": true,
    "builds": [
      {
        "src": "server/index.ts",
        "use": "@vercel/node"
      },
      {
        "src": "client/index.html",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "server/index.ts"
      },
      {
        "src": "/(.*)",
        "dest": "client/index.html"
      }
    ]
  }
  